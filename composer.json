{
	"name": "apigenerator/apigenerator.org",
	"description": "Generate apidoc as service hook and push back as github pages into repository.",
	"keywords": ["github", "apidoc", "apigen", "phpdoc", "phpdocumentor"],
	"license": "LGPL-3.0+",
	"authors": [
		{
			"name": "Tristan Lins",
			"email": "tristan.lins@bit3.de",
			"homepage": "http://bit3.de",
			"role": "Developer"
		}
	],
	"support": {
		"email": "info@apigenerator.org",
		"issues": "https://github.com/apigenerator/apigenerator.org/issues",
		"source": "https://github.com/apigenerator/apigenerator.org"
	},
	"repositories": [
		{
			"type": "git",
			"url": "https://github.com/kdambekalns/PHP-Token-Reflection.git"
		}
	],
	"require": {
		"andrewsville/php-token-reflection": "dev-scalar-expression-constants as 1.4.x-dev",

		"monolog/monolog": "~1",
		"symfony/yaml": "2.7.*",
		"symfony/process": "2.7.*",
		"symfony/filesystem": "2.3.*",
		"symfony/console": "2.7.*",
		"guzzlehttp/guzzle": "~6",
		"apigen/apigen": "~4",
		"phpdocumentor/phpdocumentor": "~2"
	},
	"autoload": {
		"psr-4": {
			"ApiGeneratorOrg\\": "src"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.x-dev"
		}
	}
}
